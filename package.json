{
  "name": "@asf/launcher",
  "version": "0.1.0",
  "description": "ASF Swarm Launcher - CLI orchestration for dashboard and agent processes",
  "main": "dist/launcher/index.js",
  "types": "dist/launcher/index.d.ts",
  "bin": {
    "asf-swarm": "./dist/bin/asf-swarm.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/asf-swarm.js",
    "dev": "npx ts-node src/bin/asf-swarm.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "asf",
    "launcher",
    "swarm",
    "orchestration",
    "cli"
  ],
  "author": "ASF Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "commander": "^12.1.0",
    "ajv": "^8.17.1",
    "tree-kill": "^1.2.2",
    "chalk": "^5.3.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "jest": "^30.0.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
